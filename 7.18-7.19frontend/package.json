    "axios": "^1.4.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "5.2.0",
    "react-toastify": "^9.1.3",
    "redux": "^5.0.1"
  },
  "jest": {
    "collectCoverage": true,
